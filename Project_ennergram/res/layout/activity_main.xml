<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="fill_parent"
              android:layout_height="fill_parent"
              android:orientation="vertical" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0.44" >

        <LinearLayout
            android:id="@+id/subLayout3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="invisible" >

            <ScrollView
                android:id="@+id/scrollView2"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textView_help"
                        android:layout_width="fill_parent"
                        android:layout_height="464dp"
                        android:text="3번 화면"
                        android:textAppearance="?android:attr/textAppearanceLarge" />

                </LinearLayout>
            </ScrollView>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/subLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="invisible" >

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2번 화면"
                android:textAppearance="?android:attr/textAppearanceLarge" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/subLayout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="invisible" >

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1번 화면"
                android:textAppearance="?android:attr/textAppearanceLarge" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mainLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="visible" >

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.92" >

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >

                    <LinearLayout
                        android:id="@+id/studySelect0"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:visibility="visible" >

                        <TextView
                            android:id="@+id/textView1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="처음 화면"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <TextView
                            android:id="@+id/textView5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Enneagram  검사"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <Button
                            android:id="@+id/Button_enneagram"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@android:drawable/btn_star"
                            android:text="검사 시작" />

                        <TextView
                            android:id="@+id/TextView01"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Enneagram  검사 결과"
                            android:textAppearance="?android:attr/textAppearanceLarge" />

                        <Button
                            android:id="@+id/Button_result"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/abc_btn_radio_material"
                            android:text="검사 결과" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/studySelect1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:clipToPadding="false"
                        android:orientation="vertical"
                        android:visibility="invisible" >

                        <ScrollView
                            android:id="@+id/ScrollView01"
                            android:layout_width="match_parent"
                            android:layout_height="243dp" >

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:orientation="vertical" >

                                <TextView
                                    android:id="@+id/TextView_ennergram"
                                    android:layout_width="match_parent"
                                    android:layout_height="241dp"
                                    android:text="문항 출력창"
                                    android:textAppearance="?android:attr/textAppearanceLarge" />
                            </LinearLayout>
                        </ScrollView>

                        <RadioGroup
                            android:id="@+id/radioGroup1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="70dp"
                            android:orientation="vertical"
                            android:paddingRight="60dp" >

                            <RadioButton
                                android:id="@+id/radio_ennergram1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:checked="false"
                                android:gravity="center_vertical"
                                android:text="전혀 아니다"
                                android:textSize="20sp"
                                android:typeface="serif" />

                            <RadioButton
                                android:id="@+id/radio_ennergram2"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="그렇지 않다"
                                android:textSize="20dp" />

                            <RadioButton
                                android:id="@+id/radio_ennergram3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="그렇다"
                                android:textSize="20dp" />

                            <RadioButton
                                android:id="@+id/radio_ennergram4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="매우 그렇다"
                                android:textSize="20sp" />

                        </RadioGroup>

                        <Button
                            android:id="@+id/buttoninsert1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:text="다음 문제" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/studySelect2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:visibility="invisible" >

                        <ScrollView
                            android:id="@+id/scrollView1"
                            android:layout_width="match_parent"
                            android:layout_height="372dp" >

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical" >

                                <TextView
                                    android:id="@+id/textView_result"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="결과 출력창"
                                    android:textAppearance="?android:attr/textAppearanceLarge" />
                            </LinearLayout>
                        </ScrollView>

                        <Button
                            android:id="@+id/buttoninsert2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:text="요약보기" />

                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <Button
                android:id="@+id/button1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:text="처음" />

            <Button
                android:id="@+id/button2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="결과" />

            <Button
                android:id="@+id/button3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="요약" />

            <Button
                android:id="@+id/button4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="도움말" />
        </LinearLayout>
    </FrameLayout>

</LinearLayout>